<%= form_tag bpm_process_definition_bpm_process_instances_path(params[:bpm_process_definition_id]), method: :post, class: 'bpm-form' do %>
  <% @form_data.each do |f| %>
    <div>
      <div>
        <%= label_tag f['id'], f['name'], class: 'bpm-form-label' %>
        <%= text_field_tag "form[#{f['id']}]", nil, class: 'bpm-form-field' %>
      </div>
    </div>
  <% end %>
  <%= submit_tag t('start') %>
<% end %>
